{
  "name": "pda-server",
  "version": "1.0.0",
  "description": "Server - PDA",
  "main": "app.js",
  "scripts": {
    "delete:log": "del log\\* /s /q",
    "delete:dist": "del dist\\* /s /q",
    "lint": "eslint src/**/*.ts",
    "nodemon:watch:js": "nodemon --watch dist --ext js ./dist/app.js",
    "compile:ts": "npm run delete:dist && npm run lint && tsc",
    "compile:prod:ts": "npm run delete:dist && npm run lint && tsc --project tsconfig.prod.json",
    "watch:ts": "watch \"npm run compile:ts\" src",
    "server:dev": "cross-env NODE_ENV=development concurrently \"npm run watch:ts\" \"npm run nodemon:watch:js\"",
    "server:prod": "cross-env NODE_ENV=production npm run compile:prod:ts node ./dist/app.js"
  },
  "author": "FAMO",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "http-status": "^1.4.2",
    "moment": "^2.29.1",
    "node-windows": "^0.1.14",
    "redis": "^2.8.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^12.19.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "concurrently": "^5.3.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "watch": "^1.0.2"
  }
}
